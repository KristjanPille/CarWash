@model Domain.Wash

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Wash</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="checkID" class="control-label"></label>
                <input asp-for="checkID" class="form-control" />
                <span asp-validation-for="checkID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="washTypeID" class="control-label"></label>
                <input asp-for="washTypeID" class="form-control" />
                <span asp-validation-for="washTypeID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="orderID" class="control-label"></label>
                <select asp-for="orderID" class ="form-control" asp-items="ViewBag.orderID"></select>
            </div>
            <div class="form-group">
                <label asp-for="nameOfWashType" class="control-label"></label>
                <input asp-for="nameOfWashType" class="form-control" />
                <span asp-validation-for="nameOfWashType" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
