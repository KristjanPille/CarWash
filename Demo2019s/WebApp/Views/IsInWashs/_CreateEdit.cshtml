@model Domain.IsInWash

<div asp-validation-summary="All" class="text-danger"></div>
<div class="form-group">
    <label asp-for="CarId" class="control-label"></label>
    <select asp-for="CarId" class ="form-control" asp-items="ViewBag.CarId"></select>
</div>
<div class="form-group">
    <label asp-for="PersonId" class="control-label"></label>
    <select asp-for="PersonId" class ="form-control" asp-items="ViewBag.PersonId"></select>
</div>
<div class="form-group">
    <label asp-for="WashId" class="control-label"></label>
    <select asp-for="WashId" class ="form-control" asp-items="ViewBag.WashId"></select>
</div>
<div class="form-group">
    <label asp-for="From" class="control-label"></label>
    <input asp-for="From" class="form-control" />
    <span asp-validation-for="From" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="To" class="control-label"></label>
    <input asp-for="To" class="form-control" />
    <span asp-validation-for="To" class="text-danger"></span>
</div>