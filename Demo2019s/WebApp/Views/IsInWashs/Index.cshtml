@model IEnumerable<DAL.App.DTO.IsInWash>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Car)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Person)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Wash)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.From)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.To)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Person!.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Wash!.NameOfWashType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.From)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.To)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IsInWashId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IsInWashId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IsInWashId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
